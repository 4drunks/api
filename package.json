{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build --production",
    "start": "node server.js",
    "dev": "node ace serve --watch",
    "lint": "eslint . --ext=.ts --max-warnings 0 --ext js,ts",
    "lint-fix": "eslint . --ext=.ts --max-warnings 0 --ext js,ts --fix",
    "format": "prettier --check .",
    "format-fix": "prettier --write .",
    "prepare": "husky install",
    "test": "echo 'Not yet implemented'",
    "commitlint-edit": "commitlint --edit $1",
    "commitlint": "commitlint --to HEAD",
    "cm": "cz"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.3.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "adonis-preset-ts": "^2.1.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^1.3.2",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "pino-pretty": "^5.0.2",
    "prettier": "^2.3.1",
    "typescript": "~4.2",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/core": "^5.1.8",
    "@adonisjs/lucid": "^14.2.0",
    "@adonisjs/repl": "^3.1.3",
    "luxon": "^1.28.1",
    "pg": "^8.6.0",
    "proxy-addr": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
